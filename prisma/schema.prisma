generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model datasets {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  address      String
  cid          String
  contributors String[]
  description  String
  title        String
}
